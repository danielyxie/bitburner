{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2016", "dom", "es2017.object", "es2019"],
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "types": ["cypress", "@testing-library/cypress", "node"]
  },
  "exclude": ["node_modules"]
}
